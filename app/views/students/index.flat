html {
  body {
    h1 {
      "Flatiron Students"
    }

    @students.each do |student| 
        p {
          pp "#{student.name} "
          a(href: "students/#{student.id}"){
            "Show"
          }
          pp '|'
          a(href: "students/#{student.id}"){
            "Edit"
          }          
        }
    end
  }
}